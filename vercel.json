{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/anuraghazra/github-readme-stats"
    }
  ],
  "headers": [
    {
      "source": "api/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=86400"
        }
      ]
    }
  ]
}
